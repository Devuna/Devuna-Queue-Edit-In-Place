[SOURCE]
PROPERTY:BEGIN
PRIORITY 5001
PROPERTY:END
SELF.FEQ{PROP:From} = 'Choice1|Choice2|Choice3|Choice4|Choice5'    !This could also specify another queue
SELF.FEQ{PROP:Drop} = 3                                            !limit viewable choices
SELF.FEQ{PROP:VScroll} = TRUE                                      !add a vertical scroll bar
GET(Q,CHOICE(SELF.ListBoxFeq))
CASE Q.f2
OF 'Choice1'
   SELF.FEQ{PROP:Selected} = 1
OF 'Choice2'
   SELF.FEQ{PROP:Selected} = 2
OF 'Choice3'
   SELF.FEQ{PROP:Selected} = 3
OF 'Choice4'
   SELF.FEQ{PROP:Selected} = 4
OF 'Choice5'
   SELF.FEQ{PROP:Selected} = 5
ELSE
   SELF.FEQ{PROP:Selected} = 1
END
[END]
[END]
[END]
[END]
